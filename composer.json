{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/colorbutton",
                "version": "4.18.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.18.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/panelbutton",
                "version": "4.18.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.18.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/colordialog",
                "version": "4.19.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/colordialog/releases/colordialog_4.19.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "ckeditor-plugin/colorbutton": "^4.18",
        "ckeditor-plugin/colordialog": "^4.19",
        "ckeditor-plugin/panelbutton": "^4.18",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/calendar": "^1.0@alpha",
        "drupal/cer": "^5.0@beta",
        "drupal/colorbutton": "^1.3",
        "drupal/colordialog": "^1.3",
        "drupal/config_ignore": "^2.3",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/ctools": "^3.6",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/devel_php": "^1.3",
        "drupal/draggableviews": "^2.0",
        "drupal/ds": "^3.12",
        "drupal/field_group": "^3.1",
        "drupal/geofield": "^1.31",
        "drupal/geofield_map": "^2.68",
        "drupal/image_widget_crop": "^2.3",
        "drupal/imce": "^2.4",
        "drupal/libraries": "^3.0@beta",
        "drupal/media_entity_slideshow": "^2.4",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_remote_audio": "^1.0",
        "drupal/menu_export": "^1.3",
        "drupal/metatag": "^1.16",
        "drupal/migrate_devel": "^2.0@alpha",
        "drupal/migrate_source_scald": "^1.0@RC",
        "drupal/migrate_tools": "^5.0",
        "drupal/migrate_upgrade": "^3",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/rebuild_cache_access": "^1.7",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.6",
        "drupal/sharethis": "^2.0@beta",
        "drupal/simple_block": "^1.3",
        "drupal/slick": "^2.4",
        "drupal/slick_lightbox": "^1.4",
        "drupal/term_reference_tree": "^1.0",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_tools": "^1.3",
        "drupal/twig_tweak": "^3.1",
        "drupal/webform": "^6.0",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^10.5",
        "furf/jquery-ui-touch-punch": "dev-master",
        "kint-php/kint": "^3.3",
        "npm-asset/slick-carousel": "^1.8",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
        "patches" : {
            "drupal/core": {
                "Add Views EntityReference filter to be available for all entity reference fields": "https://www.drupal.org/files/issues/2021-12-02/2429699-453-9.3.x.patch"
            },
            "drupal/colorbutton": {
                "How to integrate Color Dialog with Color Button module (plugin)": "https://www.drupal.org/files/issues/2019-11-26/colorbutton-color-dialog-integration-3033682-5.patch"
            }
        }
    },
    "require-dev": {
        "drupal/upgrade_status": "4.x-dev"
    }
}
